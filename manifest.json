{
  "manifest_version": 2,
  "name": "Registration Helper",
  "version": "1.1",
  "description": "This is a course registration helper for UI students. Icon credit to: https://www.flaticon.com/free-icon/books-stack-of-three_29302",
  "icons": {
    "16": "images/books-stack-of-three16.png",
    "32": "images/books-stack-of-three32.png",
    "48": "images/books-stack-of-three48.png",
    "128": "images/books-stack-of-three128.png"
  },
  "browser_action": {
    "default_icon": {
      "16": "images/books-stack-of-three16.png",
      "32": "images/books-stack-of-three32.png",
      "48": "images/books-stack-of-three48.png",
      "128": "images/books-stack-of-three128.png"
    },
    "default_popup": "popup.html"
  },
  "permissions": [
    "storage",
    "notifications",
    "https://courses.illinois.edu/"
  ],
  "background": {
    "scripts": ["background.js"],
    "persistent": true
  }
}